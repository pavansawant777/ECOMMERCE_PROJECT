<%- include("navbar.ejs") %>
<div class="container-fluid bg-white p-4">
    <div class="row">
        <div class="mb-3 col-md-12">
            <h3>Contact</h3>
        </div>
        <div class="col-md-12">
            <table class="table table-sm table-bordered">
                <tr>
                    <th>SrNo</th>
                    <th>Contact Name</th>
                    <th>Contact Email</th>
                    <th>Contact Message</th>
                </tr>
                <% for (var i=0;i<contact.length;i++) { %> 
                    <tr>
                        <th><%=i+1%></th>
                        <th><%=contact[i].contact_name%></th>
                        <th><%=contact[i].contact_email%></th>
                        <th><%=contact[i].contact_message%></th>
                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</div>

<%- include("footer.ejs") %>